{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/dcg-teddy.html",
      "destination": "/debtconsultantsgroup.com/debtconsultantsgroup.com/dcg-teddy.html"
    },
    {
      "source": "/dcg-savings-workshop.html",
      "destination": "/debtconsultantsgroup.com/debtconsultantsgroup.com/dcg-savings-workshop.html"
    },
    {
      "source": "/dcg-teddy",
      "destination": "/debtconsultantsgroup.com/debtconsultantsgroup.com/dcg-teddy.html"
    },
    {
      "source": "/dcg-savings-workshop",
      "destination": "/debtconsultantsgroup.com/debtconsultantsgroup.com/dcg-savings-workshop.html"
    },
    {
      "source": "/",
      "destination": "/debtconsultantsgroup.com/debtconsultantsgroup.com/index.html"
    },
    {
      "source": "/((?!api(?:/|$)).*)",
      "destination": "/debtconsultantsgroup.com/debtconsultantsgroup.com/$1"
    }
  ],
  "headers": [
    {
      "source": "/((?!api).*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
